<odoo>
    <data>
        <record id="view_attendee_course_tree" model="ir.ui.view">
            <field name="name">Attende</field>
            <field name="model">academic.attendee</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="partner_id"/>
                    <field name="session_id"/>
                </tree>
            </field>
        </record>

        <record id="academic_form_view" model="ir.ui.view">
            <field name="name">Attendee form</field>
            <field name="model">academic.attendee</field>
            <field name="arch" type="xml">
                <form>
                    <field name="name"/>
                    <field name="partner_id"/>
                    <field name="session_id"/>
                </form>
            </field>
        </record>


    </data>
</odoo>